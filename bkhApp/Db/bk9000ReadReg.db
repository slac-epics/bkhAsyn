  field(EGU,  "$(EGU="")")
  field(INP,  "@asyn($(PORT),$(N),1) LI_CREG")
  field(LOLO, "$(LOLO="")")
  field(LOW,  "$(LOW="")")
  field(HIGH, "$(HIGH="")")
  field(HIHI, "$(HIHI="")")
  field(LLSV, "$(LLSV=NO_ALARM)")
  field(LSV,  "$(LSV=NO_ALARM)")
  field(HSV,  "$(HSV=NO_ALARM)")
  field(HHSV, "$(HHSV=NO_ALARM)")
  field(SCAN, "I/O Intr")
}

